<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mybaties">
    <select id="selectUserById" parameterType="int" resultType="com.github.mybaties.User">
        select * from users where id = #{id}
    </select>

    <select id="deletetUserById" parameterType="int" >
        delete  from users where id = #{id}
    </select>

    <select id="selectAll" resultType="com.github.mybaties.User" >
        select *  from users
    </select>

    <select id="insertUser" parameterType="com.github.mybaties.User" >
       insert into users(name, age) values(#{name}, #{age});
    </select>

</mapper>